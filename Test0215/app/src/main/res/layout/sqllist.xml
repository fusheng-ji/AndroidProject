<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginTop="9dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="序号" android:id="@+id/textView14"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="用户名"
        android:id="@+id/textView19"
        android:layout_marginStart="46dp"
        android:layout_toEndOf="@+id/textView14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_alignBaseline="@+id/textView14"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="密码"
        android:id="@+id/textView20"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_toStartOf="@+id/textView21"
        android:layout_marginEnd="81dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:text="年龄"
        android:id="@+id/textView21"
        android:layout_alignTop="@+id/textView20"
        android:layout_alignParentEnd="true"/>
</RelativeLayout>
